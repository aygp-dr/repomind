# Tests directory Makefile

.PHONY: all unit integration expect clean

all: unit

unit:
	@echo "Running unit tests..."
	@sh run-tests.sh

integration:
	@echo "Integration tests not implemented yet"

expect:
	@echo "Expect tests not implemented yet"

clean:
	@echo "Cleaning test artifacts..."
	@find . -name "*.log" -delete
	@find . -name "*~" -delete